    // remove `support.enumPrototypes` from `iteratorTemplate`
    source = source.replace(matchFunction(source, 'iteratorTemplate'), function(match) {
      return match.replace(/^(?: *\/\/.*\n)* *["'] *(?:<% *)?if *\(support\.enumPrototypes\b *(?:&&|\))(.+?\}(?:\\n)?["']|[\s\S]+?<% *} *(?:%>|["'])).+/gm, '');
