 * @private
 * @param {string} source The source to inspect.
 * @returns {string} Returns the modified source.
 */
function removeSpliceObjectsFix(source) {
  return source.replace(/^(?: *\/\/.*\n)*( *)if\s*\(!support\.spliceObjects\b[\s\S]+?(?:\{\s*}|\n\1})\n/m, '');
}

